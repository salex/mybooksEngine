<div id="<%= dom_id account %>">
  <p>
    <strong>Uuid:</strong>
    <%= account.uuid %>
  </p>

  <p>
    <strong>Book:</strong>
    <%= account.book_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= account.name %>
  </p>

  <p>
    <strong>Account type:</strong>
    <%= account.account_type %>
  </p>

  <p>
    <strong>Code:</strong>
    <%= account.code %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= account.description %>
  </p>

  <p>
    <strong>Placeholder:</strong>
    <%= account.placeholder %>
  </p>

  <p>
    <strong>Contra:</strong>
    <%= account.contra %>
  </p>

  <p>
    <strong>Parent:</strong>
    <%= account.parent_id %>
  </p>

  <p>
    <strong>Level:</strong>
    <%= account.level %>
  </p>

</div>
